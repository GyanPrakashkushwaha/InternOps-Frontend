{
  "manifest_version": 3,
  "name": "InternOps",
  "version": "0.1",
  "author": "Gyan Prakash Kushwaha",
  "description": "Interns Getting Shortlisted",
  "permissions": [
    "activeTab",
    "storage",
    "scripting",
    "sidePanel" 
  ],
  "side_panel": {
    "default_path": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources" : [
    {
      "resources": [],
      "matches" : ["<all_urls>"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "assets/logo-removebg.png",
      "24": "assets/logo-removebg.png",
      "32": "assets/logo-removebg.png"
    },
    "default_title": "Open InternOps Side Panel"
  }
}